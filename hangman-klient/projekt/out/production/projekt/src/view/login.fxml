<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import java.net.URL?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="view.LoginController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <TextField fx:id="textFieldLogin" alignment="CENTER" layoutX="55.0" layoutY="125.0" prefHeight="54.0" prefWidth="297.0" promptText="LOGIN" />
        <PasswordField fx:id="passwordField" alignment="CENTER" layoutX="55.0" layoutY="198.0" prefHeight="54.0" prefWidth="297.0" promptText="PASSWORD" />
        <Button fx:id="buttonSignIn" alignment="CENTER" layoutX="35.0" layoutY="408.0" mnemonicParsing="false" onAction="#singIn" prefHeight="54.0" prefWidth="325.0" styleClass="bigButton" text="sign in" />
        <Button fx:id="buttonSignUp" layoutX="278.0" layoutY="558.0" mnemonicParsing="false" onAction="#signUp" prefHeight="41.0" prefWidth="96.0" styleClass="smallButton" text="sign up" />
        <CheckBox layoutX="88.0" layoutY="273.0" mnemonicParsing="false" text="Auto logg in" />
        <Label layoutX="41.0" layoutY="568.0" prefHeight="21.0" prefWidth="212.0" text="Don't have an account ?" />
        <Separator layoutX="41.0" layoutY="552.0" prefHeight="0.0" prefWidth="325.0" />
        <Label fx:id="labelInfo" alignment="CENTER" contentDisplay="CENTER" layoutX="52.0" layoutY="315.0" prefHeight="19.0" prefWidth="297.0" styleClass="infoLabel" />
    </children>
    <stylesheets>
        <URL value="@style.css" />
    </stylesheets>

</AnchorPane>
